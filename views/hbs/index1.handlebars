<h1>Datos personales</h1>
<div class="container">
  <table class="table table-striped table-bordered table-hover table-dark">
    <thead>
      <tr>
        <th>Id</th>
        <th>Titulo</th>
        <th>Precio</th>
        <th>Foto</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td>{{this.thumbnail}}</td>
      </tr>
    </tbody>
  </table>
</div>